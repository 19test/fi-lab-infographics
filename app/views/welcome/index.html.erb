<% users = 325 %>
<section id="frontpage">
  <article id="presentation">

  <section class="media">
	<!--<a class="pull-left" href="#">
      		<img class="media-object" alt="identify" src="/assets/user-front.png">
	</a>
    	<div class="media-body">
